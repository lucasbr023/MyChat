<div id="get_out_team_modal" class="modal">
  <div class="modal-content">
    <div class="row">
      <%= form_tag("get_out/0", :method => :get_out, class: "col s12 get_out_team_form") do %>
        <div class="row">
          <div class="col s12">
            <h4>Get out Team</h4>
            <p>Are you sure you want to get out the team?</p>
          </div>
          <div class="input-field col s12">
            <%= submit_tag("Get out", class: "btn red right") %>
          </div>
        </div>
      <% end %>
    </div>
  </div>
</div>
